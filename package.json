{
  "name": "ts-serverless-framework",
  "version": "1.0.0",
  "description": "Base framework for typescript lambda apps",
  "private": true,
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "start": "sls offline",
    "test-unit": "mocha -r ts-node/register tests/Unit/**/*.test.ts",
    "test-acceptance": "mocha -r ts-node/register tests/Acceptance/**/*.test.ts",
    "full-tests": "mocha -r ts-node/register tests/**/*.test.ts",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "code-doc": "node_modules/.bin/typedoc --out ./code-doc ./src"
  },
  "dependencies": {
    "@types/node": "^10.12.18",
    "aws-sdk": "2.381.*",
    "bunyan": "^1.8.12",
    "deep-equal": "^1.0.1",
    "global": "^4.3.2",
    "json-schema-ref-parser": "^6.0.2",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "serverless": "^1.35.1",
    "ts-loader": "^5.3.3"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.13",
    "@types/aws-sdk": "^2.7.0",
    "@types/bunyan": "^1.8.5",
    "@types/chai": "^4.1.7",
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^5.2.5",
    "@types/request": "2.47.0",
    "@types/request-promise-native": "1.0.14",
    "@types/sinon": "^7.0.3",
    "chai": "^4.2.0",
    "expect.js": "^0.3.1",
    "grunt": ">=1.3.0",
    "grunt-shell": "^1.1.2",
    "mocha": "^5.2.0",
    "serverless-offline": "^3.32.1",
    "serverless-plugin-typescript": "^1.1.5",
    "sinon": "^7.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.0",
    "typescript": "^3.2.2"
  }
}
